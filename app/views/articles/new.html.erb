<h1>New Article</h1>

<%= form_for @article do |f| %>
  <div class="field">
    <%= f.label :url, 'URL' %><br>
    <%= f.text_field :url %>
  </div>
  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
