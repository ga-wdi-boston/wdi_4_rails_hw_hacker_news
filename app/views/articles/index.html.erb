<h1>All Articles</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Url</th>
    <th>Create At</th>
  </tr>
  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.url %></td>
      <td><%= article.created_at %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'Add Article', new_article_path %>
