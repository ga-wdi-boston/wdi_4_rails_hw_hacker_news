<h1>All Articles</h1>

<table>
  <tr>
    <th>ID</th>
    <th>Title</th>
    <th>Article Page</th>
    <th>Url</th>
    <th>Created At</th>
  </tr>
  <% @articles.each do |article| %>
    <tr>
      <td><%= article.id %></td>
      <td><%= link_to article.title, article.url, target: '_blank' %></td>
      <td><%= link_to 'Article', article_path(article) %></td>
      <td><%= article.url %></td>
      <td><%= article.created_at %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'Add Article', new_article_path %>
